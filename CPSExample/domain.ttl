# baseURI: http://cps.dd/domain
# imports: http://dramaticallydecayed.com/core2ed

@prefix : <http://cps.dd/domain#> .
@prefix core2ed: <http://dramaticallydecayed.com/core2ed#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://cps.dd/domain>
  rdf:type owl:Ontology ;
  spin:imports <http://topbraid.org/spin/owlrl-all> ;
  owl:imports <http://dramaticallydecayed.com/core2ed> ;
.
:AmbulanceStation
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStation ;
.
:Area
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
:CityBirhdayWeek
  rdf:type :Period ;
  :periodEnd "2018-06-07T23:00:00"^^xsd:dateTime ;
  :periodStart "2018-06-01T00:00:00"^^xsd:dateTime ;
.
:CityBirthdayWeekSecurityService
  rdf:type :LocalSecurity ;
  :hasExpandability 1500 ;
  :hasMaintenanceLack 4000 ;
  :hasMaintenanceReserve 500 ;
  :maintainsPopulation 4000 ;
.
:CityServiceStation
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStationType ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :maintainService ;
      owl:someValuesFrom :LocalService ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onProperty :maintainsDistrict ;
      owl:someValuesFrom :Area ;
    ] ;
.
:CityServiceStationType
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
:CityWest
  rdf:type :Area ;
  :areaRadius "2000"^^xsd:float ;
  :covers :CityWestSquare ;
  :covers :NotrhWestDistrict ;
  :covers :SouthWestDistrict ;
  :pupolation 10000 ;
.
:CityWestPerformace
  rdf:type :PerfomanceEventCluster ;
  :consistOf :CraftFair ;
  :coversArea :CityWest ;
  :hasPeriod :CityBirhdayWeek ;
  :maintainedBy :CityBirthdayWeekSecurityService ;
.
:CityWestSquare
  rdf:type :Location ;
  :pupolation 200 ;
.
:CraftFair
  rdf:type :LivingEvent ;
  :hasLocation :CityWestSquare ;
  :hasPeriod :JohnLivingPeriod ;
.
:Event
  rdf:type owl:Class ;
  rdfs:subClassOf [
      rdf:type owl:Class ;
      owl:intersectionOf (
          [
            rdf:type owl:Restriction ;
            owl:onProperty :hasLocation ;
            owl:someValuesFrom :Location ;
          ]
          [
            rdf:type owl:Restriction ;
            owl:onProperty :hasPeriod ;
            owl:someValuesFrom :Period ;
          ]
        ) ;
    ] ;
.
:EventCluster
  rdf:type owl:Class ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          [
            rdf:type owl:Restriction ;
            owl:onProperty :coversArea ;
            owl:someValuesFrom :Area ;
          ]
          [
            rdf:type owl:Restriction ;
            owl:onProperty :hasPeriod ;
            owl:someValuesFrom :Period ;
          ]
        ) ;
    ] ;
.
:FireStation
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStation ;
.
:John
  rdf:type :Participant ;
  :headsTowards :CityWestSquare ;
  :takePartIn :CraftFair ;
.
:JohnLivingPeriod
  rdf:type :Period ;
  :periodEnd "2018-06-07T23:00:00"^^xsd:dateTime ;
  :periodStart "2018-06-01T00:00:00"^^xsd:dateTime ;
.
:LivingEvent
  rdf:type owl:Class ;
  rdfs:subClassOf :Event ;
.
:LivingEventCluster
  rdf:type owl:Class ;
  rdfs:subClassOf :EventCluster ;
.
:LocalFireSafty
  rdf:type owl:Class ;
  rdfs:subClassOf :LocalService ;
.
:LocalHealthcare
  rdf:type owl:Class ;
  rdfs:subClassOf :LocalService ;
.
:LocalSecurity
  rdf:type owl:Class ;
  rdfs:subClassOf :LocalService ;
.
:LocalService
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
:Location
  rdf:type owl:Class ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange owl:rational ;
      owl:onProperty :locationLatitude ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange owl:rational ;
      owl:onProperty :locationLongitude ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
:NorthWestPoliceStation
  rdf:type :PoliceStation ;
  :hasExpandability 800 ;
  :hasMaintenanceReserve 200 ;
  :maintainSecurity :CityWestPerformace ;
  :maintainService :CityBirthdayWeekSecurityService ;
  :maintainsDistrict :NotrhWestDistrict ;
  :maintainsPopulation 2000 ;
.
:NotrhWestDistrict
  rdf:type :Area ;
  :pupolation 2000 ;
.
:Participant
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          :Person
          [
            rdf:type owl:Restriction ;
            owl:onProperty :takePartIn ;
            owl:someValuesFrom :PerformanceEvent ;
          ]
        ) ;
    ] ;
.
:PerfomanceEventCluster
  rdf:type owl:Class ;
  rdfs:subClassOf :EventCluster ;
.
:PerformanceEvent
  rdf:type owl:Class ;
  rdfs:subClassOf :Event ;
.
:Period
  rdf:type owl:Class ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange owl:rational ;
      owl:onProperty :periodEnd ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:onDataRange owl:rational ;
      owl:onProperty :periodStart ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
.
:Person
  rdf:type owl:Class ;
.
:PoliceStation
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStation ;
.
:SouthWestDistrict
  rdf:type :Area ;
  :pupolation 2000 ;
.
:SouthWestPoliceStation
  rdf:type :PoliceStation ;
  :hasExpandability 700 ;
  :hasMaintenanceReserve 300 ;
  :maintainSecurity :CityWestPerformace ;
  :maintainService :CityBirthdayWeekSecurityService ;
  :maintainsDistrict :SouthWestDistrict ;
  :maintainsPopulation "2000"^^xsd:string ;
.
:Stationary
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStationType ;
.
:Temporary
  rdf:type owl:Class ;
  rdfs:subClassOf :CityServiceStationType ;
.
:Tourist
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          :Person
          [
            rdf:type owl:Restriction ;
            owl:onProperty :actAs ;
            owl:someValuesFrom :Visitor ;
          ]
          [
            rdf:type owl:Restriction ;
            owl:onProperty :actAs ;
            owl:someValuesFrom :Participant ;
          ]
          [
            rdf:type owl:Restriction ;
            owl:onProperty :takePartIn ;
            owl:someValuesFrom :PerformanceEvent ;
          ]
          [
            rdf:type owl:Restriction ;
            owl:onProperty :takePartIn ;
            owl:someValuesFrom :LivingEvent ;
          ]
        ) ;
    ] ;
.
:Visitor
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [
      rdf:type owl:Class ;
      owl:intersectionOf (
          :Person
          [
            rdf:type owl:Restriction ;
            owl:onProperty :takePartIn ;
            owl:someValuesFrom :LivingEvent ;
          ]
        ) ;
    ] ;
.
:actAs
  rdf:type rdf:Property ;
.
:areaRadius
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Area ;
  rdfs:range xsd:float ;
.
:assignedTo
  rdf:type owl:ObjectProperty ;
  rdfs:domain :LocalService ;
  rdfs:range :EventCluster ;
.
:consistOf
  rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
.
:covers
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Area ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          :Location
          :Area
        ) ;
    ] ;
.
:coversArea
  rdf:type owl:ObjectProperty ;
.
:hasExpandability
  rdf:type owl:DatatypeProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          :CityServiceStation
          :LocalService
        ) ;
    ] ;
  rdfs:range xsd:integer ;
.
:hasLocation
  rdf:type owl:ObjectProperty ;
.
:hasMaintenanceLack
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :LocalService ;
  rdfs:range xsd:integer ;
.
:hasMaintenanceReserve
  rdf:type owl:DatatypeProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          :CityServiceStation
          :LocalService
        ) ;
    ] ;
  rdfs:range xsd:integer ;
.
:hasPeriod
  rdf:type owl:ObjectProperty ;
.
:headsTowards
  rdf:type rdf:Property ;
.
:locationLatitude
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range xsd:double ;
.
:locationLongitude
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Location ;
  rdfs:range xsd:double ;
.
:maintainSecurity
  rdf:type owl:ObjectProperty ;
  rdfs:domain :PoliceStation ;
  rdfs:range :EventCluster ;
.
:maintainService
  rdf:type owl:ObjectProperty ;
.
:maintainedBy
  rdf:type owl:ObjectProperty ;
.
:maintainsDistrict
  rdf:type owl:ObjectProperty ;
.
:maintainsPopulation
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:integer ;
.
:periodEnd
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Period ;
  rdfs:range xsd:dateTime ;
.
:periodStart
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :Period ;
  rdfs:range xsd:dateTime ;
.
:pupolation
  rdf:type owl:DatatypeProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          :Area
          :Location
        ) ;
    ] ;
  rdfs:range xsd:integer ;
.
:takePartIn
  rdf:type owl:ObjectProperty ;
.
