<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:afn="http://jena.hpl.hp.com/ARQ/function#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:fn="http://www.w3.org/2005/xpath-functions#"
    xmlns:sp="http://spinrdf.org/sp#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:arg="http://spinrdf.org/arg#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://dramaticallydecayed.com/core2ed#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:spl="http://spinrdf.org/spl#"
  xml:base="http://dramaticallydecayed.com/core2ed">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
    <owl:imports rdf:resource="http://spinrdf.org/spl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="ComputationEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DesignEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="State">
    <rdfs:subClassOf rdf:resource="#ComputationEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="LevelGeneration">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DevelopmentEntity"/>
    </rdfs:subClassOf>
    <spin:rule>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <sp:Optional>
            <sp:elements rdf:parseType="Collection">
              <rdf:Description>
                <sp:subject rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >level</sp:varName>
                </sp:subject>
                <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                <sp:object>
                  <owl:Class rdf:ID="Level"/>
                </sp:object>
              </rdf:Description>
            </sp:elements>
          </sp:Optional>
          <sp:Filter>
            <sp:expression>
              <sp:not>
                <sp:arg1>
                  <sp:bound>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >level</sp:varName>
                    </sp:arg1>
                  </sp:bound>
                </sp:arg1>
              </sp:not>
            </sp:expression>
          </sp:Filter>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >ns</sp:varName>
            </sp:variable>
            <sp:expression>
              <afn:namespace>
                <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
              </afn:namespace>
            </sp:expression>
          </sp:Bind>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >firstLevel</sp:varName>
            </sp:variable>
            <sp:expression>
              <sp:iri>
                <sp:arg1>
                  <fn:concat>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >ns</sp:varName>
                    </sp:arg1>
                    <sp:arg2>level_1</sp:arg2>
                  </fn:concat>
                </sp:arg1>
              </sp:iri>
            </sp:expression>
          </sp:Bind>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >firstLevel</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object>
              <owl:Class rdf:about="#Level"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >firstLevel</sp:varName>
            </sp:subject>
            <sp:predicate>
              <owl:DatatypeProperty rdf:ID="hasNumber"/>
            </sp:predicate>
            <sp:object rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
            >1</sp:object>
          </rdf:Description>
        </sp:templates>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        ># generates first level</rdfs:comment>
      </sp:Construct>
    </spin:rule>
    <spin:rule>
      <sp:Construct>
        <sp:where rdf:parseType="Collection">
          <sp:SubQuery>
            <sp:query>
              <sp:Select>
                <sp:where rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >level</sp:varName>
                    </sp:subject>
                    <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sp:object>
                      <owl:Class rdf:about="#Level"/>
                    </sp:object>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:subject rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >level</sp:varName>
                    </sp:subject>
                    <sp:predicate>
                      <owl:DatatypeProperty rdf:about="#hasNumber"/>
                    </sp:predicate>
                    <sp:object rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >number</sp:varName>
                    </sp:object>
                  </rdf:Description>
                </sp:where>
                <sp:resultVariables rdf:parseType="Collection">
                  <rdf:Description>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >level</sp:varName>
                  </rdf:Description>
                  <rdf:Description>
                    <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >number</sp:varName>
                  </rdf:Description>
                </sp:resultVariables>
                <sp:limit rdf:datatype="http://www.w3.org/2001/XMLSchema#long"
                >1</sp:limit>
                <sp:orderBy rdf:parseType="Collection">
                  <sp:Desc>
                    <sp:expression rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >number</sp:varName>
                    </sp:expression>
                  </sp:Desc>
                </sp:orderBy>
              </sp:Select>
            </sp:query>
          </sp:SubQuery>
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >node</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object>
              <owl:Class rdf:ID="Node"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >node</sp:varName>
            </sp:subject>
            <sp:predicate>
              <owl:ObjectProperty rdf:ID="belongs2Level"/>
            </sp:predicate>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >level</sp:varName>
            </sp:object>
          </rdf:Description>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >ns</sp:varName>
            </sp:variable>
            <sp:expression>
              <afn:namespace>
                <sp:arg1 rdf:resource="http://spinrdf.org/spin#_this"/>
              </afn:namespace>
            </sp:expression>
          </sp:Bind>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >newNumber</sp:varName>
            </sp:variable>
            <sp:expression>
              <sp:add>
                <sp:arg1 rdf:parseType="Resource">
                  <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >number</sp:varName>
                </sp:arg1>
                <sp:arg2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >1</sp:arg2>
              </sp:add>
            </sp:expression>
          </sp:Bind>
          <sp:Bind>
            <sp:variable rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >newLevel</sp:varName>
            </sp:variable>
            <sp:expression>
              <sp:iri>
                <sp:arg1>
                  <fn:concat>
                    <sp:arg1 rdf:parseType="Resource">
                      <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >ns</sp:varName>
                    </sp:arg1>
                    <sp:arg2>
                      <fn:concat>
                        <sp:arg1>level_</sp:arg1>
                        <sp:arg2 rdf:parseType="Resource">
                          <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                          >newNumber</sp:varName>
                        </sp:arg2>
                      </fn:concat>
                    </sp:arg2>
                  </fn:concat>
                </sp:arg1>
              </sp:iri>
            </sp:expression>
          </sp:Bind>
        </sp:where>
        <sp:templates rdf:parseType="Collection">
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >newLevel</sp:varName>
            </sp:subject>
            <sp:predicate rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
            <sp:object>
              <owl:Class rdf:about="#Level"/>
            </sp:object>
          </rdf:Description>
          <rdf:Description>
            <sp:subject rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >newLevel</sp:varName>
            </sp:subject>
            <sp:predicate>
              <owl:DatatypeProperty rdf:about="#hasNumber"/>
            </sp:predicate>
            <sp:object rdf:parseType="Resource">
              <sp:varName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >newNumber</sp:varName>
            </sp:object>
          </rdf:Description>
        </sp:templates>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        ># generates new level when the max level has been linked with a node</rdfs:comment>
      </sp:Construct>
    </spin:rule>
  </owl:Class>
  <owl:Class rdf:ID="WorldEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ModelEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ComputabilityEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DesignEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Event">
    <rdfs:subClassOf rdf:resource="#State"/>
  </owl:Class>
  <owl:Class rdf:ID="ComputationUnit">
    <rdfs:subClassOf rdf:resource="#ComputabilityEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#Node">
    <rdfs:subClassOf rdf:resource="#ComputationEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#Level">
    <rdfs:subClassOf rdf:resource="#ComputationEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="ProactiveEntity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ModelEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Computed">
    <rdfs:subClassOf rdf:resource="#WorldEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#ModelEntity">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Known">
    <rdfs:subClassOf rdf:resource="#WorldEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="Percepted">
    <rdfs:subClassOf rdf:resource="#WorldEntity"/>
  </owl:Class>
  <owl:Class rdf:ID="CUWorlGeneration">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#DevelopmentEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#DesignEntity">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="SituationEntity">
    <rdfs:subClassOf rdf:resource="#ModelEntity"/>
  </owl:Class>
  <owl:Class rdf:about="#DevelopmentEntity">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="associative">
    <rdfs:range rdf:resource="#ModelEntity"/>
    <rdfs:domain rdf:resource="#ModelEntity"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="mapping_world2computability"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="situation">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="model"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#belongs2Level">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="computation"/>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="#Node"/>
    <rdfs:range rdf:resource="#Level"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasDerivative">
    <rdfs:domain rdf:resource="#Node"/>
    <rdfs:domain rdf:resource="#ComputationUnit"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="mapping_cbility2ction"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#mapping_cbility2ction">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="computability"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#computation">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="design"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#computability">
    <rdfs:subPropertyOf rdf:resource="#design"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="generative">
    <rdfs:range rdf:resource="#ModelEntity"/>
    <rdfs:domain rdf:resource="#ModelEntity"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="#mapping_world2computability"/>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#mapping_world2computability">
    <rdfs:subPropertyOf rdf:resource="#computability"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasBase">
    <rdfs:domain rdf:resource="#Node"/>
    <rdfs:domain rdf:resource="#ComputationUnit"/>
    <rdfs:subPropertyOf rdf:resource="#mapping_cbility2ction"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="proactive">
    <rdfs:subPropertyOf rdf:resource="#model"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="world">
    <rdfs:subPropertyOf rdf:resource="#model"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="#hasNumber">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#Level"/>
    <rdfs:subPropertyOf rdf:resource="#computation"/>
  </owl:DatatypeProperty>
</rdf:RDF>

