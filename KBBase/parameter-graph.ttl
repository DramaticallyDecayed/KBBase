# baseURI: http://dramaticallydecayed.com/parameter-graph
# imports: http://dramaticallydecayed.com/core2ed
# imports: http://dramaticallydecayed.com/parameterized-system

@prefix : <http://dramaticallydecayed.com/parameter-graph#> .
@prefix arg: <http://spinrdf.org/arg#> .
@prefix core2ed: <http://dramaticallydecayed.com/core2ed#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix parameterized-system: <http://dramaticallydecayed.com/parameterized-system#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix spl: <http://spinrdf.org/spl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://dramaticallydecayed.com/parameter-graph>
  rdf:type owl:Ontology ;
  owl:imports <http://dramaticallydecayed.com/core2ed> ;
  owl:imports <http://dramaticallydecayed.com/parameterized-system> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:computedClassGeneration
  rdf:type core2ed:ComputedClassGeneration ;
.
:cuWorldGeneration
  rdf:type core2ed:CUWorldGeneration ;
.
:dfGeneration
  rdf:type core2ed:DFGeneration ;
.
:elaborationGeneration
  rdf:type core2ed:ElaborationGeneration ;
.
:levelGeneration
  rdf:type core2ed:LevelGeneration ;
.
:ndGeneration
  rdf:type core2ed:NDGeneration ;
.
parameterized-system:ConcentrateParameterRule
  rdfs:subClassOf core2ed:Percepted ;
.
parameterized-system:Graph
  rdfs:subClassOf core2ed:Complex ;
.
parameterized-system:IntervalRule
  rdfs:subClassOf core2ed:Percepted ;
.
parameterized-system:ParameterizedConcept
  rdfs:subClassOf core2ed:WorldHLConcept ;
.
parameterized-system:SystemParameter
  rdfs:subClassOf core2ed:Known ;
.
parameterized-system:concentrateParameterRuleFor
  rdfs:subPropertyOf core2ed:complex ;
  rdfs:subPropertyOf core2ed:generative ;
.
parameterized-system:includeInterval
  rdfs:subPropertyOf core2ed:generative ;
  owl:propertyChainAxiom (
      parameterized-system:intervalRule
      parameterized-system:produceIntervals
    ) ;
.
parameterized-system:intervalRule
  rdfs:subPropertyOf core2ed:associative ;
.
parameterized-system:linearParameterFor
  rdfs:subPropertyOf core2ed:complex ;
  rdfs:subPropertyOf core2ed:generative ;
  owl:propertyChainAxiom (
      parameterized-system:includeInterval
      parameterized-system:linearInervalFor
    ) ;
.
parameterized-system:parameterConcentratorFor
  rdfs:subPropertyOf core2ed:complex ;
  rdfs:subPropertyOf core2ed:generative ;
  owl:propertyChainAxiom (
      parameterized-system:includeInterval
      parameterized-system:intervalConcentratorFor
    ) ;
.
parameterized-system:parameterizedProperty
  rdfs:subPropertyOf core2ed:world ;
.
