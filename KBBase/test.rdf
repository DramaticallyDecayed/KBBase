        <rdf:type rdf:resource="#Point"/>
        <rdf:type rdf:resource="#Player"/>
      </VisibleObject>
    </see>
    <see>
      <VisibleObject rdf:ID="ball">
        <rdf:type rdf:resource="#Point"/>
        <rdf:type rdf:resource="#Ball"/>
      </VisibleObject>
    </see>
